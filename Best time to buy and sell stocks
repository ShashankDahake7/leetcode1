class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maximumProfit=0;
        int minimum=prices[0];
        for(int i=0;i<prices.size();i++){
            minimum=min(minimum,prices[i]);
            int profit=prices[i]-minimum;
            maximumProfit=max(maximumProfit,profit);
        }
        return maximumProfit;
    }
};
