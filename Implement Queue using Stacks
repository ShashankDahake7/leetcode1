class MyQueue {
private:
    stack<int> s1,s2;
public:
    MyQueue() {}
    void push(int x) {
        s1.push(x);
    }
    int pop() {
        if(s2.empty()){
            while(!s1.empty()){
                s2.push(s1.top());
                s1.pop();
            }
        }   
        if(s2.empty()){
            return -1;
        }
        int x = s2.top();
        s2.pop();
        return x;
    }
    int peek() {
        if(s2.empty()){
            while(!s1.empty()){
                s2.push(s1.top());
                s1.pop();
            }
        }
        if(s2.empty()){
            return -1;
        }
        return s2.top();
    }
    bool empty() {
        if(s1.empty() && s2.empty()){
            return 1;
        }    
        else{
            return 0;
        }
    }
};
