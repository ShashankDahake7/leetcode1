class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int n = nums.size();
        int ans;
        if(n<0)
        {
            return -1;
        }
        if(n<=2)
        {
            
            return nums[0];
        }
        sort(nums.begin() , nums.end());
        for(int i = 0 ; i<n ; i++)
        {
            if (nums[i] == nums[i+1])
            {
                 ans = nums[i] ;
            }
        }
        return ans;
    }
};
